set.seed(154)
x.b1 <- c(rnorm(10, mean = 5, sd = 1),
rnorm(10, mean = 6.5, sd = 0.15),
rnorm(10, mean = 5.1, sd = 0.7))
x.b2 <- c(rnorm(10, mean = 4, sd = 1),
rnorm(10, mean = 5.2, sd = 0.15),
rnorm(10, mean = 4.5, sd = 0.7))
bloque <- rep(c(1, 2, each = 30))
tratamiento <- c(rep(c(1, 2, 3), each = 10),
rep(c(1, 2, 3), each = 10))
base <- data.frame(Valor = c(x.b1, x.b2),
Bloque = bloque,
Tratamiento = tratamiento)
library(ggplot2)
ggplot(base) + geom_boxplot(aes(x = Tratamiento, y = Valor))
ggplot(base) + geom_boxplot(aes(x = Tratamiento, y = Valor, fill = Tratamiento))
